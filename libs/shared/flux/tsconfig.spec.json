{
	"extends": "./tsconfig.json",
	"compilerOptions": {
		"outDir": "../../../dist/out-tsc",
		"jsx": "react-jsx",
		"module": "commonjs",
		"types": [
			"jest",
			"node"
		],
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true
	},
	"include": [
		"jest.config.ts",
		"src/**/*.test.ts",
		"src/**/*.spec.ts",
		"src/**/*.d.ts"
	]
}
